{
  "name": "cycle-google-maps",
  "version": "0.0.2",
  "description": "CycleJS driver for Google Maps V3 using jsondiffpatch",
  "jsnext:main": "./lib/es6/main.js",
  "main": "./lib/cjs/main.js",
  "esnext:types": "./lib/es6/main.d.ts",
  "types": "./lib/cjs/main.d.ts",
  "scripts": {
    "build": "tsc",
    "dist": "rollup --config rollup.config.cjs.js",
    "test": "rollup --config rollup.config.test.js -w & sleep 8; live-server --port=4000",
    "prepublish": "npm run clean; npm run build; npm run dist",
    "clean": "rm -rf lib; rm -rf dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ntilwalli/cycle-google-maps.git"
  },
  "keywords": [
    "cyclejs",
    "google-maps",
    "jsondiffpatch"
  ],
  "author": "Nikhil Tilwalli",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ntilwalli/cycle-google-maps/issues"
  },
  "homepage": "https://github.com/ntilwalli/cycle-google-maps#readme",
  "dependencies": {
    "@cycle/rxjs-adapter": "^3.0.3",
    "jsondiffpatch": "^0.2.4",
    "rxjs": "^5.2.0"
  },
  "devDependencies": {
    "@cycle/dom": "^14.0.0",
    "@cycle/rxjs-run": "^3.0.3",
    "@types/chai": "^3.4.34",
    "@types/core-js": "^0.9.34",
    "@types/mocha": "^2.2.32",
    "@types/google-maps": "^3.2.0",
    "@types/geojson": "^1.0.0",
    "chai": "^3.5.0",
    "mocha": "^3.1.0",
    "rollup-plugin-commonjs": "^8.0.0",
    "rollup-plugin-json": "^2.1.0",
    "rollup-plugin-node-builtins": "^2.1.0",
    "rollup-plugin-node-globals": "^1.1.0",
    "rollup-plugin-node-resolve": "^2.0.0",
    "rollup-plugin-typescript": "^0.8.1",
    "typescript": "^2.2.0",
    "xstream": "^7.0.0"
  }
}
